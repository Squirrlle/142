#include <iostream>
#include<map>
#include<fstream>
using std::map;
using std::cout;
using std::endl;
using std::string;
using std::getline;

struct Book {

	string author;
	int yearPublished;

};

typedef map<string, Book> BookMap;

void read_books(BookMap m_books, string file_name){

	string title, authors;
	int year;
	ifstream(file_name);

	getline(file_name, title);

}

template<typename T, class List>
void print(List ls, T n) {
	if (n > ls.size) {
		for (T o : ls) {
			cout << o << endl;
		}
	} else {

		for (auto itr = ls.begin(); itr < ls.begin() + n; itr++) {
			cout << itr << endl;
		}

	}
}
;

int main() {
	return 0;
}
